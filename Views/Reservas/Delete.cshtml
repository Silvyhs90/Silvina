@model tpc5FI.Models.Reserva

@{
    ViewData["Title"] = "Cancelar";
}

<h1>Cancelar</h1>

<h3>Estas Seguro que quieres cancelar esta reserva?</h3>
<div>
    <h4>Reserva</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.FDesde)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.FDesde)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.FHasta)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.FHasta)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.codigoAlojamiento)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.codigoAlojamiento)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.codigoPersona)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.codigoPersona)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.precio)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.precio)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="id" />
        <input type="submit" value="Cancelar" class="btn btn-danger" /> 
    </form>
</div>
