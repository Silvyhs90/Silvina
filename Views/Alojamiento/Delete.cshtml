@model tpc5FI.Models.Alojamiento

@{
    ViewData["Title"] = "Eliminar";
}

<h1>Eliminar</h1>

<h3>Estas seguro de eliminar este Alojamiento?</h3>
<div>
    <h4>Alojamiento</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.tipo)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.tipo)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.ciudad)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.ciudad)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.barrio)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.barrio)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.estrellas)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.estrellas)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.cantPersonas)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.cantPersonas)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.tv)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.tv)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.precioxDia)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.precioxDia)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.precioxPersona)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.precioxPersona)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.habitacion)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.habitacion)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.baño)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.baño)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="codigo" />
        <input type="submit" value="Eliminar" class="btn btn-danger" />
        <a asp-action="Index" class="btn btn-primary">Cancelar</a>
    </form>
</div>
